{
    "name": "jurjean/spray-persistence-bundle",
    "require": {
        "doctrine/orm": "@stable",
        "doctrine/doctrine-bundle": "@stable",
        "symfony/dependency-injection": "@stable",
        "symfony/http-kernel": "@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "doctrine/doctrine-fixtures-bundle": "2.2.*@dev",
        "jms/serializer-bundle": "0.13.*@dev"
    },
    "suggest": {
        "jms/serializer-bundle": "Allows serialization of RepositoryFilter"
    },
    "autoload": {
        "psr-0": {
            "Spray\\PersistenceBundle": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2.x-dev"
        }
    }
}
